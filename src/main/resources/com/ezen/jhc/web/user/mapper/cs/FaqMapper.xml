<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.jhc.web.user.mapper.cs.FaqMapper">

	<select id="get" resultType="com.ezen.jhc.web.user.dto.faq.FaqDTO">
		SELECT * FROM faqs WHERE faq_ctgr = #{faq_ctgr} ORDER BY faq_num
	</select> 
	
	<select id="get_ctgr" resultType="com.ezen.jhc.web.user.dto.faq.FaqDTO">
		SELECT faq_ctgr FROM faqs
	</select> 
</mapper>